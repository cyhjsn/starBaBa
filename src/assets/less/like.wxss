.like {
  padding-top: 46px;
}
.like /deep/ .van-nav-bar .van-icon {
  color: #106e33;
}
.like /deep/ .van-nav-bar__text {
  color: #106e33;
}
.like .bg-content {
  padding-bottom: 0 !important;
}
.like .like-item {
  width: calc(100% / 3 - 10px + 10px / 3);
  margin-right: 10px;
  margin-bottom: 10px;
}
.like .like-item:nth-child(3n) {
  margin-right: 0;
}
.like .pro-name {
  margin-top: 10px;
  font-size: 14px;
}
.like .one-text {
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.like .pro-enname {
  margin-top: 5px;
  margin-bottom: 10px;
  font-size: 12px;
  color: #999;
}
.like .pro-price {
  font-size: 14px;
  color: #106e33;
  font-weight: bold;
}
.like .delete {
  width: 15px;
  height: 15px;
  margin-top: 2px;
  color: #999;
}
